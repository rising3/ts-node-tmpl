{
  "name": "ts-node-tmpl",
  "version": "0.1.0",
  "description": "TypeScript and Node.js template",
  "main": "index.js",
  "license": "MIT",
  "author": "Michio Nakagawa",
  "repository": {
    "type": "git",
    "url": "https://github.com/rising3/ts-node-tmpl.git"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "yarn clean && yarn lint:fix && yarn tsc && yarn tsc-alias && yarn typedoc",
    "start": "yarn build && node dist/src/index.js",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev:watch": "ts-node-dev --respawn --poll -r tsconfig-paths/register src/index.ts",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "eslint 'src/**/*.[jt]s?(x)'",
    "lint:fix": "eslint 'src/**/*.[jt]s?(x)' --fix",
    "typedoc": "typedoc --out ./dist/docs/ ./src/"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.1",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "4",
    "@typescript-eslint/parser": "^4.14.0",
    "autoprefixer": "^10.2.4",
    "babel-jest": "^26.6.3",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^5.1.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.6",
    "postcss-loader": "^5.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.32.7",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "tsc-alias": "^1.2.2",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.20.16",
    "typescript": "^4.1.5",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-node-externals": "^2.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged --allow-empty"
    }
  },
  "lint-staged": {
    "./src/*.{js,ts,json}": "yarn eslint --cache --fix"
  },
  "dependencies": {
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "eslint-plugin-react": "^7.22.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
